<!DOCTYPE html>
<html class="no-js" {{ site.language_attributes }} dir="ltr">
    <head>
        {% include 'particals/head.twig' %}
    </head>
    <body class="{{ body_class }}">
        {% include 'particals/header.twig' %}
        <section id="content" role="main" class="content-wrapper">
            <div class="wrapper">
                {% block content %}
                    Sorry, no content
                {% endblock %}
            </div>
        </section>

        {% block modals %}
            {% include 'particals/modals.twig' %}
        {% endblock %}

        {% block footer %}
            {% include 'particals/footer.twig' %}
            {{ fn('wp_footer') }}
        {% endblock %}

        {% block javascripts %}
            {% include 'particals/devs.twig' %}
        {% endblock %}
    </body>
</html>
