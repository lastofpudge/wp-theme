{
  "name": "theme/blank",
  "type": "wp-theme",
  "version": "2.0.10",
  "description": "Wordpress starter theme with Timber/Twig",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Andrii Dudin",
      "email": "iluxor1991@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*"
      ]
    }
  ],
  "keywords": [
    "WordPress",
    "starter",
    "timber"
  ],
  "require": {
    "php": "7.4|8.*",
    "htmlburger/carbon-fields": "^3.6",
    "timber/timber": "^2.0.0",
    "twig/twig": "^v3.8.0",
    "wp-theme/core": "^1.0.4",
    "wpackagist-plugin/all-in-one-wp-migration": "*",
    "wpackagist-plugin/wordpress-seo": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "App\\Admin\\": "app/Admin"
    },
    "files": [
      "bootstrap/app.php",
      "app/helpers.php",
      "app/Handlers/ajax.php",
      "app/CarbonFields/OptionFields.php",
      "app/CarbonFields/PostFields.php",
      "app/CarbonFields/Blocks.php",
      "routes/api.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform-check": false,
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "scripts": {
    "lint": [
      "pint --preset psr12"
    ]
  },
  "extra": {
    "installer-paths": {
      "../../plugins/{$name}": [
        "wpackagist-plugin/all-in-one-wp-migration",
        "wpackagist-plugin/wordpress-seo"
      ]
    }
  },
  "require-dev": {
    "laravel/pint": "^1.13",
    "php-stubs/wordpress-stubs": "^6.4"
  }
}
