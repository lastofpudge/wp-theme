{% extends 'layouts/base.twig' %}

{% block content %}
    {% if fn('function_exists', 'yoast_breadcrumb') %}
        {{ fn('yoast_breadcrumb', '<nav id="breadcrumbs" class="main-breadcrumbs">','</nav>', false ) }}
    {% endif %}

    <div class="post-wrap" id="post-{{ post.ID }}">
        <h1>
            {{ post.title }}
        </h1>

        {{ post.content }}
    </div>
{% endblock %}
